{
  "appId": "com.bichobom",
  "appName": "Bicho Bom",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "cordova": {},
  "plugins": {
    "CapacitorFirebaseAuth": {
      "providers": [
        "google.com",
        "facebook.com"
      ],
      "languageCode": "en",
      "nativeAuth": false,
      "permissions": {
        "google": [
          "profile"
        ],
        "facebook": [
          "profile"
        ]
      }
    }
  },
  "server": {
    "url": "http://192.168.0.24:8100",
    "cleartext": true
  }
}
