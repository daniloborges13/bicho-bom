{
  "appId": "com.bichobom",
  "appName": "bichoBom",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "cordova": {},
  "plugins": {
    "CapacitorFirebaseAuth": {
      "providers": [
        "google.com",
        "facebook.com"
      ],
      "nativeAuth": false,
      "languageCode": "en",
      "permissions": {
        "google": [
          "profile",
          "https://www.googleapis.com/auth/drive"
        ]
      }
    }
  },
  "server": {
    "url": "http://localhost:8100",
    "cleartext": true
  }
}
